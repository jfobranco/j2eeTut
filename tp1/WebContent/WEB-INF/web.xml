<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>tp1</display-name>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
    </jsp-property-group>
  </jsp-config>
  <servlet>
    <servlet-name>ClientControl</servlet-name>
    <servlet-class>com.jb.servlets.ClientControl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ClientControl</servlet-name>
    <url-pattern>/creationClient</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>OrderControl</servlet-name>
    <servlet-class>com.jb.servlets.OrderControl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OrderControl</servlet-name>
    <url-pattern>/creationCommande</url-pattern>
  </servlet-mapping>
</web-app>